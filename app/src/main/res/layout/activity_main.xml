<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textEqual"
        android:layout_width="324dp"
        android:layout_height="77dp"
        android:layout_marginBottom="36dp"
        android:text="Ian Nelson"
        android:textAlignment="textEnd"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="132dp"
        android:text="CWU ID: 41746296"
        android:textAlignment="textEnd"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.734"
        app:layout_constraintStart_toStartOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="413dp"
        android:layout_height="480dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonA"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="0"
                    android:backgroundTint="#FF8033"
                    android:onClick="onClickA"
                    android:text="AC"
                    android:textSize="24sp"
                    android:translationY="-6dp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonL"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClickL"
                    android:text="("
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonR"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClickR"
                    android:text=")"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonDiv"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#AFEA7B"
                    android:onClick="onClickDiv"
                    android:text="/"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:backgroundTint="#EABD7B" />

                <Button
                    android:id="@+id/Button7"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick7"
                    android:text="7"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button8"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick8"
                    android:text="8"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button9"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick9"
                    android:text="9"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonMul"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#AFEA7B"
                    android:onClick="onClickMul"
                    android:text="*"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button4"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick4"
                    android:text="4"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button5"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick5"
                    android:text="5"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button6"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick6"
                    android:text="6"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonSub"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#AFEA7B"
                    android:onClick="onClickSub"
                    android:text="-"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button1"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick1"
                    android:text="1"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button2"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick2"
                    android:text="2"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button3"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClick3"
                    android:text=":3"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonAdd"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#AFEA7B"
                    android:onClick="onClickAdd"
                    android:text="+"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />
            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/Button0"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#E033FF"
                    android:onClick="onClick0"
                    android:text="0"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonDec"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#EABD7B"
                    android:onClick="onClickDec"
                    android:text="."
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonC"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#337DFF"
                    android:onClick="onClickC"
                    android:text="C"
                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/ButtonEq"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_weight="1"
                    android:backgroundTint="#AFEA7B"
                    android:onClick="onClickEq"
                    android:text="="

                    android:textSize="36sp" />

                <Space
                    android:layout_width="10.2dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />
            </LinearLayout>

        </TableRow>
    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>